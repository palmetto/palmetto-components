{
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", { "modules": "cjs" }],
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/plugin-transform-regenerator",
        "@babel/plugin-transform-runtime"
      ]
    },
    "build": {
       "presets": [
         ["@babel/preset-env", { "modules": false }],
         "@babel/preset-react",
         "@babel/preset-typescript"
        ]
     }
  }
}
