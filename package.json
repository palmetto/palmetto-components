    "semantic-release": "semantic-release",
    "generate-readme-toc": "yarn doctoc docs/CONTRIBUTING.md",
    "test:watch": "cross-env BABEL_ENV=test jest --watch --verbose",
    "type-check": "tsc --pretty",
    "type-check:watch": "tsc --watch --pretty"
  },
